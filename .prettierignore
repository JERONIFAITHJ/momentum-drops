dist/
build/
out/
.next/ # For Next.js

# Ignore Node.js modules
node_modules/

# Ignore log files
*.log
npm-debug.log*
yarn-debug.log*
pnpm-debug.log*

# Ignore editor/IDE files
.vscode/
.idea/
*.sublime-project
*.sublime-workspace

# Ignore environment variables
.env
.env.development
.env.production
.env.local
.env.*.local

# Ignore OS-generated files
.DS_Store
Thumbs.db

# Ignore coverage reports
coverage/

# Ignore temporary files
*.tmp
